<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.dickiezulkarnaen.gogoncell">

    <!-- To auto-complete the email text field in the login form with the user's emails -->
    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="android.permission.READ_PROFILE" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/icongogon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/icongogon"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity"
            android:label="Gogon Cell">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".pulsa"
            android:label="PULSA" />
        <activity
            android:name=".MenuUtama"
            android:label="GOGON CELL" />
        <activity
            android:name=".kuota"
            android:label="PAKET DATA" />
        <activity
            android:name=".telkomsel"
            android:label="PAKET DATA TELKOMSEL" />
        <activity
            android:name=".indosat"
            android:label="PAKET DATA INDOSAT" />
        <activity
            android:name=".xl"
            android:label="PAKET DATA XL" />
        <activity
            android:name=".three"
            android:label="PAKET DATA THREE" />
        <activity
            android:name=".axis"
            android:label="PAKET AXIS" />
        <activity
            android:name=".smart"
            android:label="PAKET DATA SMARTFREN" />
        <activity
            android:name=".bolt"
            android:label="PAKET DATA BOLT"></activity>
    </application>

</manifest>